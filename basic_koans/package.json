{
  "name": "typescript-basic-koans",
  "version": "1.0.0",
  "description": "Learn TypeScript basics through test-driven koans",
  "main": "lib/path-to-enlightenment.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "generate": "node lib/generate-koans.js",
    "test": "yarn build && yarn build:koans && node koans/lib/path-to-enlightenment.js",
    "test:watch": "node ../watch-test.js",
    "watch": "yarn test:watch",
    "walk": "yarn test",
    "clean": "rm -rf koans/ lib/",
    "reset": "yarn clean && yarn generate",
    "install:koans": "cd koans && yarn install",
    "build:koans": "cd koans && tsc",
    "test:koans": "cd koans && yarn test",
    "clean:koans": "cd koans && rm -rf lib"
  },
  "keywords": [
    "typescript",
    "basic",
    "learning",
    "koans",
    "tdd",
    "education"
  ],
  "author": "TypeScript Koans Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
